<?xml version='1.0' encoding='UTF-8'?>

<faces-config
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>pc_Header</managed-bean-name>
		<managed-bean-class>pagecode.layout.Header</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_LoginLayout</managed-bean-name>
		<managed-bean-class>pagecode.layout.LoginLayout</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_MenuLateral</managed-bean-name>
		<managed-bean-class>pagecode.layout.MenuLateral</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_Logout</managed-bean-name>
		<managed-bean-class>pagecode.pages.publiques.Logout</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_FormLayout</managed-bean-name>
		<managed-bean-class>pagecode.layout.FormLayout</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_AltaExpedient</managed-bean-name>
		<managed-bean-class>pagecode.pages.protegides.AltaExpedient</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_Principal</managed-bean-name>
		<managed-bean-class>pagecode.pages.protegides.Principal</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_Cercador</managed-bean-name>
		<managed-bean-class>pagecode.pages.protegides.Cercador</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_DetallsExpedient</managed-bean-name>
		<managed-bean-class>pagecode.pages.protegides.DetallsExpedient</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pc_Login</managed-bean-name>
		<managed-bean-class>pagecode.pages.publiques.Login</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<from-view-id>/pages/protegides/altaExpedient.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/pages/protegides/detallsExpedient.xhtml</to-view-id>
			<redirect include-view-params="true"></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>neteja</from-outcome>
			<to-view-id>/pages/protegides/altaExpedient.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/protegides/cercador.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/pages/protegides/cercador.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>selecciona</from-outcome>
			<to-view-id>/pages/protegides/detallsExpedient.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/pages/protegides/detallsExpedient.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/pages/protegides/detallsExpedient.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cercador</from-outcome>
			<to-view-id>/pages/protegides/cercador.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>alta</from-outcome>
			<to-view-id>/pages/protegides/altaExpedient.xhtml</to-view-id>
			<redirect include-view-params="true"></redirect>
		</navigation-case>
	</navigation-rule>
</faces-config>